//to find the num is even and odd

import java.util.Scanner;
public class Main{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int a;
        a=sc.nextInt();
        /*if(a%2 == 0)
        {
            System.out.println("even number");
        }
        else{
            System.out.println("odd number");
        }*/
        if((a | 1)> a)
        {
            System.out.println("even number");
        }
        else{
            System.out.println("odd number");
        }
    }
}
